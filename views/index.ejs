    <!-- START THE FEATURETTES -->
    <div class="container">
      <hr class="featurette-divider">
      <div class="row featurette">
        <div class="col-md-7">
          <h2 class="featurette-heading fw-normal lh-1 heading"> First featurette heading. <span
              class="text-body-secondary heading">It'll blow your mind.</span></h2>
          <br>
          <p class="lead">
            <%= sampleText %>
          </p>
          <%= sampleText.substring(0,400) + "..." %>
        </div>
        <div class="col-md-5">
