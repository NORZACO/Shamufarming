  <header>
    <!-- Add your logo image here -->
    <a href="#">
      <img src="https://th.bing.com/th/id/R.03c6f5e4b79226f03154e2daf54c78f2?rik=yEnOQ9Nnz%2fhuLg&riu=http%3a%2f%2flogos-download.com%2fwp-content%2fuploads%2f2016%2f04%2fNY_Yankees_logo.png&ehk=8pTHhv2VA%2fS619FGbJ38jKsWPCnrv3gHLfA19u73Snw%3d&risl=&pid=ImgRaw&r=0" height="100" alt="Logo" class="logo">
    </a>

    <input type="checkbox" id="nav-toggle" class="nav-toggle">
    <nav>
      <ul>
        <li><a href="#"><i class="fas fa-home"></i> Home</a></li>
        <li><a href="#"><i class="fas fa-info-circle"></i> About</a></li>
        <li><a href="#"><i class="fas fa-blog"></i> Blog</a></li>
        <li><a href="#"><i class="fas fa-envelope"></i> Contact</a></li>
        <li><a href="#"><i class="fas fa-store"></i> Store</a></li>
        <% if (user) { %>
          <li>
            <a href="/users/<%= user._id %>">
              <i class="fas fa-user"></i> <%= user.username %>
            </a>
         </li>
         <!-- logout post form -->
          <li style="color: aliceblue;">
            <form action="/logout" method="POST" >
              <button class="btn btn-link nav-link px-2 link-body-emphasisx" type="submit">
                <i class="fas fa-sign-out-alt"></i> Logout
              </button>
            </form>
          </li>

        <% } else if (!user) { %>
          <li><a href="/login"> <i class="bi bi-box-arrow-in-left"></i> Login </a></li>
        <% } %>




      </ul>
    </nav>
    <label for="nav-toggle" class="nav-toggle-label">
      <span></span>
    </label>
  </header>